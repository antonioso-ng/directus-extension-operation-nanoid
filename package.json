{
	"name": "directus-extension-operation-nanoid",
	"description": "Generate a NanoID in your Directus flows.",
	"type": "module",
	"version": "1.0.2",
	"license": "MIT",
	"icon": "extension",
	"homepage": "https://github.com/antonioso-ng/directus-extension-operation-nanoid.git#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/antonioso-ng/directus-extension-operation-nanoid.git"
	},
	"bugs": {
		"url": "https://github.com/antonioso-ng/directus-extension-operation-nanoid/issues"
	},
	"author": {
		"name": "Antonio Song",
		"email": "hello@antonioso.ng"
	},
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-operation",
		"operation-nanoid",
		"nanoid"
	],
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "operation",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"source": {
			"app": "src/app.ts",
			"api": "src/api.ts"
		},
		"host": "^10.10.0",
		"hidden": false,
		"sandbox": {
			"enabled": true,
			"requestedScopes": {}
		}
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.1.0",
		"@types/node": "^22.7.7",
		"typescript": "^5.6.3",
		"vue": "^3.5.12"
	},
	"dependencies": {
		"nanoid": "^5.0.7"
	},
	"packageManager": "bun@1.1.31"
}
